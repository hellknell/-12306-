<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DAO" targetRuntime="MyBatis3">
        <!--覆盖生成的mapper.xml文件-->
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin"/>
        <plugin type="com.heyu.mybatis.generator.plugin.MapperPlugin"/>
        <plugin type="com.heyu.mybatis.generator.plugin.ListPlugin"/>
        <plugin type="com.heyu.mybatis.generator.plugin.InitModelDefaultValuePlugin"/>
        <plugin type="com.heyu.mybatis.generator.plugin.FieldAttributePlugin">
        </plugin>
        <plugin type="com.heyu.mybatis.generator.plugin.ExampleWhereClausePlugin">
        </plugin>
        <plugin type="com.heyu.mybatis.generator.plugin.TopOnePlugin">
        </plugin>
        <plugin type="com.heyu.mybatis.generator.plugin.CountPlugin">
        </plugin>
        <plugin type="com.heyu.mybatis.generator.plugin.UpdateFieldPlugin">
        </plugin>
        <plugin type="com.heyu.mybatis.generator.plugin.UpdateByExampleWhereClausePlugin">
        </plugin>
        <plugin type="com.heyu.mybatis.generator.plugin.UpdateFieldBatchPlugin">
        </plugin>
        <plugin type="com.heyu.mybatis.generator.plugin.GetPlugin">
        </plugin>
        <plugin type="com.heyu.mybatis.generator.plugin.UpdateBatchSelectivePlugin">
        </plugin>
        <commentGenerator type="com.heyu.mybatis.generator.plugin.GeneratorCommentPlugin">
            <!-- 是否去除所有自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
            <!-- 是否添加数据库内的注释 true：是 ： false:否 -->
            <property name="addRemarkComments" value="true"/>
            <!-- 是否去除自动生成日期的注释 true：是 ： false:否 -->
            <property name="suppressDate" value="true"/>
            <property name="author" value="heyu"/>
        </commentGenerator>

        <jdbcConnection
                driverClass="com.mysql.cj.jdbc.Driver"
                connectionURL="jdbc:mysql://localhost:3306/train_batch?useUnicode=true&amp;useSSL=false&amp;characterEncoding=utf8&amp;allowPublicKeyRetrieval=true&amp;serverTimezone=Asia/Shanghai"
                userId="train_batch"
                password="qweASDzx11"
        >
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>
        <javaModelGenerator
                targetPackage="com.heyu.train.batch.domain"
                targetProject="E:\IdeaProhect\train12306\batch\src\main\java"
        />
        <!--自动生成的*Mapper.xml文件存放路径 (src/main/resources/文件夹一定要提前建好，否则不生成文件) -->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="E:\IdeaProhect\train12306\batch\src\main\resources">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <!--mapper接口生成位置 -->
        <javaClientGenerator
                type="XMLMAPPER"
                targetPackage="com.heyu.train.batch.mapper"
                targetProject="E:\IdeaProhect\train12306\batch\src\main\java">
            <property name="enableSbPackages" value="false"/>
        </javaClientGenerator>

    </context>
</generatorConfiguration>

